package ArrayList;
import java.util.ArrayList;

public class ArrayLIst
{
    public static void main(String[] args)
    {
        ArrayList<Integer> n =  new ArrayList<Integer>();
        n.add(10);
        n.add(20);
        n.add(30);

        n.add(3,45);// first = index, 2nd = value

        System.out.print(n);
        System.out.println();

        for(int  i : n)
        {
            System.out.print(i+" ");
        }
    }
}
